{
  "name": "dear-mep",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:test-page": "http-server ./test-page -p 8080",
    "build": "npm run build:ng && npm run build:bundle",
    "build:ng": "ng build",
    "build:bundle": "node ./scripts/bundle-assets.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "generate-client": "ng-openapi-gen --input 'http://127.0.0.1:8000/openapi.json' --output src/app/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.0.0",
    "@angular/elements": "^15.2.1",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.2.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@ngneat/transloco": "^4.2.2",
    "@ngneat/transloco-locale": "^4.0.0",
    "concat": "^1.0.3",
    "fs-extra": "^11.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-openapi-gen": "^0.24.0",
    "typescript": "~4.8.2"
  }
}